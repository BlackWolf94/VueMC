{
  "name": "@zidadindimon/vue-mc",
  "version": "0.0.2",
  "description": "vue model/collection system",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "typings": "lib/index.d.ts",
  "bin": {
    "docts": "docts"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run prepare && npm version patch",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "docs": "npm run prepare && docts"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/gwynblaid/vue-mc.git"
  },
  "author": "Dmitro Zataidukh <zidadindimon@gmail.com>",
  "license": "ISC",
  "homepage": "https://bitbucket.org/gwynblaid/vue-mc#readme",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "docts": "^0.2.0",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typedoc": "^0.13.0",
    "typedoc-markdown-theme": "0.0.4",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "@zidadindimon/js-typehelper": "^1.0.17",
    "arraysearch": "^1.2.0",
    "deepmerge": "^2.2.1",
    "vue": "^2.5.17"
  }
}
