    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "faker": "^5.5.3",
    "jest": "~26.6.3",
    "jest-html-reporters": "^2.1.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tplant": "^2.3.6",
    "ts-jest": "~26.5.5",
    "typedoc": "^0.20.35",
    "typescript": "~4.2.4",
    "vuepress": "^1.8.2",
    "vuetify": "^2.4.11"
