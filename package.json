    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "tplant": "^2.3.3",
    "ts-jest": "~25.5.1",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.4",
    "typedoc-markdown-theme": "0.0.4",
