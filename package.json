    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@zidadindimon/js-typehelper": "^1.0.17",
    "copyfiles": "~2.4.1",
    "docsify-cli": "~4.4.0",
    "docts": "~0.2.0",
    "eslint": "^7.22.0",
