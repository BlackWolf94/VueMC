    "@types/jest": "^24.0.23",
    "docts": "^0.2.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.13.0",
    "typedoc-markdown-theme": "0.0.4",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "@types/node": "^12.12.14",
    "@zidadindimon/js-typehelper": "^1.0.17",
    "vue": "^2.6.10"
