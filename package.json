    "@zidadindimon/js-typehelper": "^1.0.17"
  },
  "devDependencies": {
    "@types/core-js": "~2.5.4",
    "@types/faker": "^5.5.3",
    "@types/jest": "~26.0.22",
    "@types/node": "~14.14.*",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.1.0",
    "copyfiles": "~2.4.1",
    "docsify-cli": "~4.4.3",
    "docts": "~0.2.0",
    "eslint": "^7.24.0",
